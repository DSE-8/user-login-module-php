{
  "name": "deg540/user-login-module",
  "description": "User login module",
  "type": "project",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.10",
    "mockery/mockery": "^1.6"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "UserLoginService\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UserLoginService\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "test": [
      "phpunit --configuration phpunit.xml"
    ]
  }
}
